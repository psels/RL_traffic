<routes>
    <!-- Vehicle Type Definition -->
    <vType id="cautious" accel="2" decel="5" sigma="0.3" length="4"
           minGap="1.5" maxSpeed="30" color="0,0,1" carFollowModel="Krauss"/>




    <!-- Route Definitions -->
    <!-- N > W -->
    <route id="route_0" edges="end1_junction junction_end4"/>
    <!-- N > S -->
    <route id="route_1" edges="end1_junction junction_end3"/>
    <!-- N > E -->
    <route id="route_2" edges="end1_junction junction_end2"/>

    <!-- W > N -->
    <route id="route_3" edges="end2_junction junction_end1"/>
    <!-- W > E -->
    <route id="route_4" edges="end2_junction junction_end4"/>
    <!-- W > S -->
    <route id="route_5" edges="end2_junction junction_end3"/>

    <!-- S > E -->
    <route id="route_6" edges="end3_junction junction_end2"/>
    <!-- S > N -->
    <route id="route_7" edges="end3_junction junction_end1"/>
    <!-- S > W -->
    <route id="route_8" edges="end3_junction junction_end4"/>

    <!-- W > S -->
    <route id="route_9" edges="end4_junction junction_end3"/>
    <!-- W > E -->
    <route id="route_10" edges="end4_junction junction_end2"/>
    <!-- W > N -->
    <route id="route_11" edges="end4_junction junction_end1"/>



    <!-- Adding Vehicles -->
    <flow id="CNW" type="cautious" route="route_0" begin="0" end="600" probability="0.1"/>


    <flow id="CNS" type="cautious" route="route_1" begin="0" end="600" probability="0.1"/>


    <flow id="CNE" type="cautious" route="route_2" begin="0" end="600" probability="0.1"/>


    <flow id="CEN" type="cautious" route="route_3" begin="0" end="600" probability="0.1"/>


    <flow id="CEW" type="cautious" route="route_4" begin="0" end="600" probability="0.1"/>

    <flow id="CES" type="cautious" route="route_5" begin="0" end="600" probability="0.1"/>

    <flow id="CSE" type="cautious" route="route_6" begin="0" end="600" probability="0.1"/>

    <flow id="CSN" type="cautious" route="route_7" begin="0" end="600" probability="0.1"/>


    <flow id="CSW" type="cautious" route="route_8" begin="0" end="600" probability="0.1"/>

    <flow id="CWS" type="cautious" route="route_9" begin="0" end="600" probability="0.1"/>

    <flow id="CWE" type="cautious" route="route_10" begin="0" end="600" probability="0.1"/>


    <flow id="CWN" type="cautious" route="route_11" begin="0" end="600" probability="0.1"/>



</routes>
